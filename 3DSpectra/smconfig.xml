		</aa>
		<aa symbol="S" id="Serine" formula="C3 H5 N1 O2" h2oloss=""
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="6.93" pkAside="" pkBside="" >
			<markerIon for="S" label="S" formula="C O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="60"/> 
		</aa>
		<aa symbol="T" id="Threonine" formula="C4 H7 N1 O2" h2oloss=""
			AChain="C1 H3" BChain="O1 H1" pkCterm="3.55" pkNterm="6.82" pkAside="" pkBside="" >
			<markerIon for="T" label="T" formula="C O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="74"/> 
		</aa>
		<aa symbol="V" id="Valine" formula="C5 H9 N1 O1" 
			AChain="C1 H3" BChain="0" pkCterm="3.55" pkNterm="7.44" pkAside="" pkBside="" >
			<markerIon for="V" label="V" formula="C O" hiCID="1" bonus="0.5" penalty="0" charge="1" ionized="1" pmni="0" nMass="72"/> 
		</aa>
		<aa symbol="W" id="Tryptophan" formula="C11 H10 N2 O1" 
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="" >
			<markerIon for="W" label="W" formula="C O" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="159"/> 
		</aa>
		<aa symbol="Y" id="Tyrosine" formula="C9 H9 N1 O2" 
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="10.0" pkBside="" >
