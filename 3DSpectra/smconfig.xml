			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="5.98" >
			<markerIon for="H" label="H" formula="C O" bonus="1.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="110"/> 
			<markerIon for="H" label="H" formula="" hiCID="1" bonus="1.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="138"/> 
		</aa>
		<aa symbol="I" id="Isoleucine" formula="C6 H11 N1 O1" 
			AChain="C1 H3" BChain="C2 H5" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="" >
			<markerIon for="IL" label="IL" formula="C O" hiCID="1" bonus="0.2222" penalty="0" charge="1" ionized="1" pmni="0" nMass="86"/> 
		</aa>
		<aa symbol="K" id="Lysine" formula="C6 H12 N2 O1" basic="" nh3loss=""
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="10.0" >
			<markerIon for="KQ" label="KQ" formula="C H3 N O" bonus="0.5" penalty="0" charge="1" ionized="1" pmni="0" nMass="84" /> 
			<markerIon for="KQ" label="KQ" formula="C O" bonus="0.5" penalty="0" charge="1" ionized="1" pmni="0" nMass="101" /> 
			<markerIon for="KQR" label="KQR" formula="" bonus="0.2" penalty="0" charge="1" ionized="1" pmni="0" nMass="129"/> 
		</aa>
		<aa symbol="L" id="Leucine" formula="C6 H11 N1 O" 
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="" >
			<markerIon for="LI" label="IL" formula="C O" bonus="0.2222" penalty="0" charge="1" ionized="1" pmni="0" nMass="86"/> 
		</aa>
		<aa symbol="M" id="Methionine" formula="C5 H9 N1 O1 S1" 
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.0" pkAside="" pkBside="" >
			<markerIon for="M" label="M" formula="C O" hiCID="1" bonus="0.5" penalty="0" charge="1" ionized="1" pmni="0" nMass="104" /> 
		</aa>
		<aa symbol="N" id="Asparagine" formula="C4 H6 N2 O2" nh3loss=""
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="" >
			<markerIon for="NR" label="NR" formula="C O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="87" /> 
		</aa>
		<aa symbol="P" id="Proline" formula="C5 H7 N1 O1" 
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="8.36" pkAside="" pkBside="" >
			<markerIon for="PR" label="PR" formula="C O" bonus="0.2" penalty="0" charge="1" ionized="1" pmni="0" nMass="70"/> 
			<!-- this is formed by the addition of a CO : -->
			<markerIon for="P" label="P" formula="C-1 O-1" hiCID="1" bonus="0.2" penalty="0" charge="1" ionized="1" pmni="0" nMass="126"/> 
		</aa>
		<aa symbol="Q" id="Glutamine" formula="C5 H8 N2 O2"	nh3loss=""
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="" >
			<markerIon for="QK" label="KQ" formula="C O2" bonus="0.5" penalty="0" charge="1" ionized="1" pmni="0" nMass="84" /> 
			<markerIon for="QK" label="KQ" formula="C O" bonus="0.5" penalty="0" charge="1" ionized="1" pmni="0" nMass="101" /> 
			<markerIon for="QKR" label="KQR" formula="" bonus="0.2" penalty="0" charge="1" ionized="1" pmni="0" nMass="129"/> 
		</aa>
		<aa symbol="R" id="Arginine" formula="C6 H12 N4 O1" basic="" nh3loss=""
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="" pkBside="12.0" >
			<markerIon for="RP" label="PR" formula="C2 H5 N3 O" bonus="0.2" penalty="0" charge="1" ionized="1" pmni="0" nMass="70"/> 
			<markerIon for="R" label="R" formula="C4 H6 N O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="73"/> 
			<markerIon for="RN" label="NR" formula="C3 H6 N2 O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="87" /> 
			<markerIon for="R" label="R" formula="C2 H3 N O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="100"/> 
