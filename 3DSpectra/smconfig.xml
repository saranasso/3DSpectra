		<aa symbol="o" id="HomoserineLactone" formula="C4 H6 N1 O2" 
			AChain="H1" BChain="0" pkCterm="" pkNterm="" pkAside="" pkBside="" />
	</aminoAcids>

	<!-- NOTES: the site attributes which are the same as with AA's are optional, in which case they inherit the same as the amino acid -->
	<!-- a type of "any" means the modifications may be "fixed" or "variable", while "cyclic" implies fixed -->
	<!-- "variable" or "any" mods may be assigned a PMFScoring attibrute, which supports the following values:
			"0" : do not score if match found
			"1" : always score if match found
			"2" : only score if unmodified match is also found
	-->
	<modifications>
		<!-- Note that peptide nterm's and cterm's may only be fixed or cyclic -->
		<mod id="pepAcetyl" name="Acetyl" type="fixed" formula="C2 H2 O">
			<site aa="*" term="n" />
		</mod>
		<mod id="Carbamyl" name="Carbamyl" type="fixed" formula="C H N O">
			<site aa="*" term="n" />
		</mod>
		<mod id="Carbamyl-Partial" name="Carbamyl-mix" type="cyclic" >
		 <site aa="*" term="n" />
			<cycles>
				<cycle n="1"></cycle>
				<cycle n="2"><cyclemod mod="Carbamyl"/> </cycle>
			</cycles>
		</mod>
		<mod id="Propionyl-D0" name="Propionyl-D0" formula="C3 O1 H4" type="fixed" DEQ="1" DEQId="Propionyl">
			<site aa="*" term="n" />
		</mod>
		<mod id="Propionyl-D5" name="Propionyl-D5" formula="C3 O1 D5 H-1" type="fixed" DEQ="2" DEQId="Propionyl">
			<site aa="*" term="n" />
		</mod>
		<mod id="Propionyl-mix" DEQ="M" name="Propionyl-mix" DEQId="Propionyl" type="cyclic" DEQType="normal">
			<site aa="*" term="n" />
			<cycles>
				<cycle n="1"> <cyclemod mod="Propionyl-D0"/></cycle>
				<cycle n="2"> <cyclemod mod="Propionyl-D5"/></cycle>
			</cycles>
		</mod>
		<!-- for iTRAQ, define just the 116 isotope, but define markers for all -->
		<!-- iTRAQ markerIon formulas are 'absolute', not 'delta' formulas -->
		<mod id="iTRAQ_116" name="iTRAQ-mix" type="fixed" iTRAQ="1" formula="C4 H12 N1 O1 c3 n1" >
			<site aa="*" term="n" />
			<site aa="K" />
			<markerIon for="*" label="iTRAQ" formula="C5 H12 N2 c1"
				bonus="0.1" penalty="0" charge="1" ionized="1" pmni="0" nMass="114" /> 
			<markerIon for="*" label="iTRAQ" formula="C5 H12 N1 c1 n1"
				bonus="0.1" penalty="0" charge="1" ionized="1" pmni="0" nMass="115" /> 
			<markerIon for="*" label="iTRAQ" formula="C4 H12 N1 c2 n1"
				bonus="0.1" penalty="0" charge="1" ionized="1" pmni="0" nMass="116" /> 
			<markerIon for="*" label="iTRAQ" formula="C3 H12 N1 c3 n1"
				bonus="0.1" penalty="0" charge="1" ionized="1" pmni="0" nMass="117" /> 
			<markerIon for="*" label="iTRAQ" formula="C2 H12 N1 c4 n1"
				bonus="0.1" penalty="0" charge="1" ionized="1" pmni="0" nMass="118" /> 
			<markerIon for="*" label="iTRAQ" formula="C4 H12 N1 O1 c3 n1"
				bonus="0.1" penalty="0" charge="1" ionized="1" pmni="0" nMass="145" /> <!-- for 116, 117 -->
		</mod>

		<!-- cterm : note that cterm's may only be fixed or cyclic -->
		<mod id="Amide" name="Amide" type="fixed">
			<site aa="*" term="c" formula="O-1 N1 H1" />
		</mod>
	 	<mod id="Amide-mix" name="Amide-mix" type="cyclic">
		 	<site aa="*" term="c" />
		 	<cycles>
			 	<cycle n="1"></cycle>
			 	<cycle n="2"><cyclemod mod="Amide" /></cycle>
		 	</cycles>
		</mod>
		<mod id="Methyl_Ester" name="Methyl Ester" formula="C H2" DEQ="1" DEQId="Methyl_Ester" type="fixed">
			<site aa="*" term="c" />
			<site aa="D" />
			<markerIon for="D" label="D" formula="C O" bonus="1.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="102" /> 
			<site aa="E" />
			<markerIon for="E" label="E" formula="C O" bonus="1.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="116" /> 
		</mod>
		<mod id="Methyl_Ester-D3" name="Methyl Ester-D3" formula="C D3 H-1" DEQ="2" DEQId="Methyl_Ester" type="fixed" >
			<site aa="*" term="c" />
			<site aa="D" />
			<markerIon for="D" label="D-D3" formula="C O" bonus="1.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="105" /> 
			<site aa="E" />
			<markerIon for="E" label="E-D3" formula="C O" bonus="1.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="119" /> 
		</mod>
		<mod id="Methyl_Ester-mix" name="Methyl Ester-mix" DEQ="M" DEQId="Methyl_Ester" type="cyclic" DEQType="normal">
			<site aa="*" term="c" />
			<site aa="D" />
			<site aa="E" />
			<cycles>
				<cycle n="1"><cyclemod mod="Methyl_Ester" /></cycle>
				<cycle n="2"><cyclemod mod="Methyl_Ester-D3"/></cycle>
			</cycles>
		</mod>
		<mod id="O_Free_Acid-O16" name="O16 Free Acid" formula="" DEQ="1" DEQId="O_Free_Acid" type="fixed" hide="1">
			<site aa="*" term="c" />
		</mod>
		<mod id="O_Free_Acid-O18" name="O18 Free Acid" formula="O-2 o2" DEQ="2" DEQId="O_Free_Acid" type="fixed">
			<site aa="*" term="c" />
		</mod>
		<mod id="O_Free_Acid-mix" name="O16/O18 Free Acid-mix" DEQ="M" type="cyclic" DEQId="O_Free_Acid"  DEQType="normal">
			<site aa="*" term="c" />
			<cycles>
				<cycle n="1"><cyclemod mod="O_Free_Acid-O16" /></cycle>
				<cycle n="2"><cyclemod mod="O_Free_Acid-O18"/></cycle>
			</cycles>
		</mod>
		<!-- cys -->
		<mod id="acrylamide" name="Acrylamide" type="any" formula="C3 H5 N1 O1" PMFScoring="1">
			<site aa="C" />
		</mod>
		<mod id="acrylamide-mix" name="Acrylamide-mix" type="cyclic">
			<site aa="C" />
			<cycles>
				<cycle n="1"></cycle>
				<cycle n="2"><cyclemod mod="acrylamide"/></cycle>
			</cycles>
		</mod>
		<mod id="carboxymethylation" name="Carboxymethylation" formula="C2 H2 O2" type="any" PMFScoring="1">
			<site aa="C" />
			<markerIon for="C" label="C" formula="C O" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="134" /> 
		</mod>
		<mod id="carbamidomethylation" name="Carbamidomethylation" formula="C2 H3 N1 O1" type="any" PMFScoring="1">
			<site aa="C" />
			<markerIon for="C" label="C" formula="C O" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="133" /> 
		</mod>
		<mod id="MMTS" name="MMTS" formula="C1 H2 S1" type="any" PMFScoring="1">
			<site aa="C" pkAside="" />
			<markerIon for="C" label="C" formula="C O" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="90" /> 
		</mod>	 
		<mod id="pyridylethylation" name="Pyridylethylation" formula="C7 H7 N1" type="any" PMFScoring="1" >
			<site aa="C" pkAside="" />
			<markerIon for="C" label="C" formula="C O" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="181" /> 
		</mod>
		<mod id="sulfoxide" name="Cysteine sulfoxide" formula="O" type="variable" PMFScoring="0">
			<site aa="C" />
			<markerIon for="C" label="C" formula="C O" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="92" /> 
		</mod>
		<mod id="ICAT-D0" name="ICAT-D0" formula="C20 H34 N4 O5 S1" type="fixed" DEQ="1" DEQId="ICAT">
			<site aa="C" pkAside="" />
			<markerIon for="C" label="C-D0" formula="C13 H21 N3 O2 S1" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="1" nMass="284" /> 
			<markerIon for="C" label="C-D0" formula="C18 H34 N4 O4 S1" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="1" nMass="403" /> 
			<markerIon for="C" label="C-D0" formula="C20 H36 N4 O5 S2" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="1" nMass="477" /> 
		</mod>
		<mod id="ICAT-D8" name="ICAT-D8" formula="C20 H26 D8 N4 O5 S1" type="fixed" DEQ="2" DEQId="ICAT">
			<site aa="C" pkAside="" />
			<markerIon for="C" label="C-D8" formula="C13 H17 D4 N3 O2 S1" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="1" nMass="288" /> 
			<markerIon for="C" label="C-D8" formula="C18 H26 D8 N4 O4 S1" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="1" nMass="411" /> 
			<markerIon for="C" label="C-D8" formula="C20 H28 D8 N4 O5 S2" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="1" nMass="485" /> 
		</mod>
		<mod id="ICAT-mix" name="ICAT-mix" DEQ="M" DEQId="ICAT" type="cyclic" DEQType="normal">
			<site aa="C" />	
			<cycles>
				<cycle n="1"><cyclemod mod="ICAT-D0" /></cycle>
				<cycle n="2"><cyclemod mod="ICAT-D8" /></cycle>
			</cycles>
		</mod>
		<mod id="cICAT-C12" name="cICAT-C12" formula="C10 H17 N3 O3" type="fixed" DEQ="1" DEQId="cICAT">
			<site aa="C" pkAside="" />
			<markerIon for="C" label="C" formula="C O" bonus="2.0" penalty="0" charge="1" ionized="1" pmni="0" nMass="303" /> 
