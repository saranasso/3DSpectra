<configurations>	

<config id="default">

	<!-- Define elements -->
	<!-- Each element may contain multiple isotope's - place main isotope first	-->
	<elements>
		<element symbol="C" valency="4">
			<isotope mass="12.0" abund="0.989" />
			<isotope mass="13.003354826" abund="0.011" />
		</element>
		<element symbol="c" valency="4" >
			<isotope mass="13.003354826" abund="1.0" />
		</element>
		<element symbol="H" valency="1" >
			<isotope mass="1.007825035" abund="0.99985" />
			<isotope mass="2.014101779" abund="0.00015" />
		</element>
			<!-- 'h' defines a proton: H - electron mass, used in formulas for cation weight as well -->
		<element symbol="h" valency="1" >
			<isotope mass="1.0072763" abund="0.99985" />
			<isotope mass="2.014101779" abund="0.00015" />
		</element>
		<element symbol="D" valency="1">
			<isotope mass="2.014101779" abund="1.0" />
		</element>
		<element symbol="N" valency="3">
			<isotope mass="14.003074002" abund="0.9963" />
			<isotope mass="15.00010897" abund="0.0037" />
		</element>
		<element symbol="n" valency="3">
			<isotope mass="15.00010897" abund="1.0" />
		</element>
		<element symbol="O" valency="2">
			<isotope mass="15.99491463" abund="0.9976" />
			<isotope mass="17.9991312" abund="0.002" />
			<isotope mass="16.9991312" abund="0.0004" />
		</element>
		<element symbol="o" valency="2">
			<isotope mass="17.9991312" abund="1.0" /> 
		</element>
		<!-- equivalent of 2 18-O's: -->
		<element symbol="j" valency="2">
			<isotope mass="20.00334777" abund="1.0" />
		</element>
		<element symbol="P" valency="3">
			<isotope mass="30.9737620" abund="1.0" />
		</element>
		<element symbol="S" valency="2">
			<isotope mass="31.972070698" abund="0.9502" />
			<isotope mass="33.967866650" abund="0.0421" />
			<isotope mass="32.971458428" abund="0.0075" />
			<isotope mass="34.971458428" abund="0.0000" />
			<isotope mass="35.967080620" abund="0.0002" />
		</element>

	</elements>	 

	<!-- Amino Acid definitions - these should be treated as read-only! -->
	<!-- DO NOT MODIFY the standard Amino Acids ! -->
	<!-- h2oLoss, nh3Loss, and basic attributes are flags - only present if 'true' -->
	<!-- markerIon notes: 
		'pmni' stands for 'parentMinusNeutralIonized'
		The nMass attribute is just there for reference -	actual mass is calculated from the formula.
		If ionized=1 and pmni=0 then the formula must not include the proton - the cation weight will be added automatically.
		In these cases the formula represents a mass that is subtracted from the (perhaps modified) aa mass.
	-->
	<aminoAcids id="standardAAs">
		<aa symbol="A" id="Alanine" formula="C3 H5 N1 O1" 
			AChain="0" BChain="0" pkCterm="3.55" pkNterm="7.59" pkAside="" pkBside="" />
		<aa symbol="C" id="Cysteine" formula="C3 H5 N1 O1 S1" 
			AChain="H1" BChain="0" pkCterm="3.55" pkNterm="7.5" pkAside="9.0" pkBside="" >
			<markerIon for="C" label="C" formula="C O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="76"/> 
		</aa>
		<aa symbol="D" id="Aspartic Acid" formula="C4 H5 N1 O3" h20loss=""
			AChain="H1" BChain="0" pkCterm="4.55" pkNterm="7.5" pkAside="4.05" pkBside="" >
			<markerIon for="D" label="D" formula="C O" hiCID="1" bonus="0.3333" penalty="0" charge="1" ionized="1" pmni="0" nMass="88"/> 
		</aa>
		<aa symbol="E" id="Glutamic Acid" formula="C5 H7 N1 O3"	h2oloss=""
